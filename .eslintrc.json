{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "next/core-web-vitals"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["simple-import-sort"],
  "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
  }
}
